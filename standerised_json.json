[[{"headword": "ma\u0323\u00b7chen", "wordclass": "<acronym title=\"verb\">VB</acronym>", "separator": "\u00b7", "flexion": "<machst, machte, hat gemacht>", "verbclass": "<acronym title=\"with object\">with obj</acronym>", "def_blocks": [{"header_num": "1. ", "grammatical_construction": "<acronym title=\"jemand\">jd</acronym> macht etwas", "definition": "(an)fertigen, produzieren", "example": ["Wer hat diesen Tisch gemacht?", "Kannst du ein Foto von uns machen?"]}, {"header_num": "2. ", "grammatical_construction": "<acronym title=\"jemand\">jd</acronym>/etwas macht etwas", "definition": "verursachen", "example": ["M\u00fcsst ihr so einen L\u00e4rm machen?", "Er hat ihr eine Freude gemacht.", "Dieser Computer macht nur Schwierigkeiten."]}, {"header_num": "3. ", "grammatical_construction": "<acronym title=\"jemand\">jd</acronym> macht etwas", "definition": "ausf\u00fchren, erledigen", "example": "Wann machst du die Hausaufgaben/die Pr\u00fcfung?"}, {"header_num": "4. ", "grammatical_construction": "<acronym title=\"jemand\">jd</acronym> macht <acronym title=\"jemanden\">jdn</acronym>/etwas irgendwie", "definition": "in einen bestimmten Zustand bringen", "example": ["Weshalb hatte sie sich so h\u00fcbsch gemacht?", "Er hat die Arbeit fertig gemacht.", "Das macht mich froh/neugierig/traurig."]}, {"header_num": "5. ", "grammatical_construction": ["<acronym title=\"jemand\">jd</acronym>/etwas macht <acronym title=\"jemanden\">jdn</acronym>/etwas irgendwie/zu <acronym title=\"jemandem\">jdm</acronym>/etwas", "<acronym title=\"jemand\">jd</acronym>/etwas macht <acronym title=\"jemanden\">jdn</acronym>/etwas zu <acronym title=\"jemandem\">jdm</acronym>/etwas"], "definition": "zu etwas werden lassen", "example": ["Die Sorgen hatten ihn ganz krank gemacht.", "Er machte den Freund zu seinem Gesch\u00e4ftspartner."]}, {"header_num": "6. ", "grammatical_construction": "<acronym title=\"jemand\">jd</acronym> macht etwas <span class=\"case\"><acronym title=\"accusative\">acc</acronym></span> ", "case": "<acronym title=\"accusative\">acc</acronym>", "definition": "sich ereignen lassen und davon profitieren", "example": ["Ich habe dabei ein gutes Gesch\u00e4ft gemacht.", "hohe Gewinne machen"]}, {"header_num": "7. ", "grammatical_construction": "<acronym title=\"jemand\">jd</acronym> macht etwas", "definition": "etwas tun", "example": ["Was habt ihr im Urlaub gemacht?", "Was machst du am Wochenende?"]}, {"header_num": "8. ", "grammatical_construction": "etwas macht etwas", "style": "<acronym title=\"informal\">inf</acronym>", "definition": "betragen, ergeben", "example": ["Das macht 27 Euro.", "Zwei mal zwei macht vier."]}, {"header_num": "9. ", "grammatical_construction": "etwas macht nichts", "style": "<acronym title=\"informal\">inf</acronym>", "definition": "etwas hat keine negativen Konsequenzen und ist nicht weiter schlimm"}]}, {"headword": "ma\u0323\u00b7chen", "wordclass": "<acronym title=\"verb\">VB</acronym>", "separator": "\u00b7", "flexion": "<machst, machte, hat gemacht>", "verbclass": "<acronym title=\"with 'sich'\">with SICH</acronym>", "def_blocks": [{"header_num": "1. ", "grammatical_construction": "<acronym title=\"jemand\">jd</acronym> macht sich (an etwas <span class=\"case\"><acronym title=\"accusative\">acc</acronym></span>)", "case": "<acronym title=\"accusative\">acc</acronym>", "definition": "anfangen", "example": "Ich mache mich dann ans Tapezieren."}, {"header_num": "2. ", "grammatical_construction": "<acronym title=\"jemand\">jd</acronym> macht sich", "style": "<acronym title=\"informal\">inf</acronym>", "definition": "sich (gut) entwickeln", "example": "Er macht sich in der Schule."}, {"header_num": "3. ", "grammatical_construction": "<acronym title=\"jemand\">jd</acronym>/etwas macht sich (irgendwie)", "definition": "eine bestimmte Rolle in der genannten Weise ausf\u00fcllen", "example": ["Das Sofa macht sich gut in dieser Ecke.", "Sie macht sich nicht schlecht als Versicherungsvertreterin."]}, {"header_num": "4. ", "grammatical_construction": "<acronym title=\"jemand\">jd</acronym> macht auf etwas <span class=\"case\"><acronym title=\"accusative\">acc</acronym></span> ", "case": "<acronym title=\"accusative\">acc</acronym>", "style": "<acronym title=\"informal\">inf</acronym>", "rhetoric": "<acronym title=\"pejorative\">pej</acronym>", "definition": "<acronym title=\"jemand\">jd</acronym> versucht in gek\u00fcnstelter Weise eine bestimmte Rolle zu spielen", "example": "Seit er bei dieser Firma ist, macht er ganz auf Manager."}, {"header_num": "5. ", "grammatical_construction": "<acronym title=\"jemand\">jd</acronym> macht in etwas <span class=\"case\"><acronym title=\"accusative\">acc</acronym></span> ", "case": "<acronym title=\"accusative\">acc</acronym>", "style": "<acronym title=\"informal\">inf</acronym>", "rhetoric": "<acronym title=\"euphemistic\">euph</acronym>", "definition": "seine Notdurft verrichten", "example": "Der Hund hat hinter den Busch gemacht."}, {"header_num": "6. ", "grammatical_construction": "<acronym title=\"jemand\">jd</acronym> macht es", "style": ["<acronym title=\"informal\">inf</acronym>", "<acronym title=\"informal\">inf</acronym>", "<acronym title=\"informal\">inf</acronym>", "<acronym title=\"informal\">inf</acronym>"], "rhetoric": "<acronym title=\"euphemistic\">euph</acronym>", "definition": "Geschlechtsverkehr haben", "example": ["Sie sagte, sie h\u00e4tten es noch am gleichen Abend gemacht.", "Was macht die Arbeit/dein Projekt/die Gesundheit?"], "idiom_proverb": ["jemand macht es nicht mehr lange", "bei einer Sache ist nichts (mehr) zu machen", "Mach dir nichts draus!", "Was macht ...?"], "sense": ["(<acronym title=\"jemand\">jd</acronym> lebt nicht mehr lange)", "(man kann nichts mehr tun, um etwas zu bewirken oder zu retten)", "(verwendet, um auszudr\u00fccken, dass sich <acronym title=\"jemand\">jd</acronym> \u00fcber etwas nicht \u00e4rgern oder sorgen soll)", "(verwendet, um zwanglos nach dem Befinden von <acronym title=\"jemandem\">jdm</acronym> oder dem Stand der Dinge in einer Angelegenheit zu fragen)"]}]}], [{"headword": "he\u0331i\u0331\u00df ma<span class=\"separator\">\u00b7</span>chen", "wordclass": "<acronym title=\"verb\">VB</acronym>", "separator": "\u00b7", "flexion": "<machst hei\u00df, machte hei\u00df, hat hei\u00dfgemacht>", "verbclass": "<acronym title=\"with object\">with obj</acronym>", "def_blocks": [{"header_num": "1. ", "grammatical_construction": "<acronym title=\"jemand\">jd</acronym> macht <acronym title=\"jemanden\">jdn</acronym> hei\u00df", "style": "<acronym title=\"informal\">inf</acronym>", "definition": "jemanden sexuell reizen", "example": "Wow! Die Frau macht mich hei\u00df!"}, {"header_num": "Phrases:", "idiom_proverb": ["<acronym title=\"jemand\">jd</acronym> macht <acronym title=\"jemandem\">jdm</acronym> die H\u00f6lle hei\u00df", "<acronym title=\"jemand\">jd</acronym> macht <acronym title=\"jemandem\">jdm</acronym> die H\u00f6lle hei\u00df"], "style": ["<acronym title=\"informal\">inf</acronym>", "<acronym title=\"informal\">inf</acronym>"], "sense": ["(<acronym title=\"jemanden\">jdn</acronym> unter Druck setzen)", "(<acronym title=\"jemanden\">jdn</acronym> unter Druck setzen)"]}, {"header_num": "Zusammenschreibung"}]}], [{"headword": "ka<span class=\"separator\">\u00b7</span>pu\u0323tt ma<span class=\"separator\">\u00b7</span>chen", "wordclass": "<acronym title=\"verb\">VB</acronym>", "separator": "\u00b7", "verbclass": "<acronym title=\"with object\">with obj</acronym>", "style": "<acronym title=\"informal\">inf</acronym>", "def_blocks": [{"header_num": "1. ", "grammatical_construction": "<acronym title=\"jemand\">jd</acronym>/etwas macht etwas/<acronym title=\"jemanden\">jdn</acronym> kaputt", "definition": "zerbrechen, zerst\u00f6ren", "example": ["Wer hat die Vase kaputtgemacht?", "Dieser Mensch macht mich mit seinem ewigen Zynismus kaputt."]}, {"header_num": "2. ", "grammatical_construction": "etwas macht <acronym title=\"jemanden\">jdn</acronym> kaputt", "definition": "bis zur v\u00f6lligen Ersch\u00f6pfung anstrengen", "example": "Der Umzug macht mich noch kaputt!"}, {"header_num": "3. ", "grammatical_construction": "etwas macht etwas kaputt", "definition": "ruinieren", "example": "Die wirtschaftliche Entwicklung macht viele kleine Betriebe kaputt."}]}, {"headword": "ka<span class=\"separator\">\u00b7</span>pu\u0323tt ma<span class=\"separator\">\u00b7</span>chen", "wordclass": "<acronym title=\"verb\">VB</acronym>", "separator": "\u00b7", "verbclass": "<acronym title=\"with 'sich'\">with SICH</acronym>", "def_blocks": [{"header_num": "", "idiom_proverb": "<acronym title=\"jemand\">jd</acronym> macht sich kaputt", "style": "<acronym title=\"informal\">inf</acronym>", "definition": "sich und seine Gesundheit ruinieren", "example": ["Er hat sich mit Drogen kaputtgemacht.", "Wenn sie weiter so viel arbeitet, macht sie sich kaputt."]}]}], [{"headword": "ku\u0323rz ma<span class=\"separator\">\u00b7</span>chen", "wordclass": "<acronym title=\"verb\">VB</acronym>", "separator": "\u00b7", "flexion": "<machst kurz, machte kurz, hat kurzgemacht>", "verbclass": "<acronym title=\"with object\">with obj</acronym>", "def_blocks": [{"header_num": "1. ", "grammatical_construction": "<acronym title=\"jemand\">jd</acronym> macht etwas <span class=\"case\"><acronym title=\"accusative\">acc</acronym></span> kurz", "case": "<acronym title=\"accusative\">acc</acronym>", "definition": "etwas k\u00fcrzen"}, {"header_num": "2. ", "grammatical_construction": "<acronym title=\"jemand\">jd</acronym> macht es kurz", "definition": "nicht viel Aufhebens von etwas machen", "example": ["Kannst du mir den Rock kurzmachen/kurz machen?", "Ich habe nicht viel Zeit, lass es uns kurzmachen!"]}, {"header_num": "Zusammen- oder Getrenntschreibung"}, {"header_num": "Zusammenschreibung"}]}], [{"headword": "Bett machen", "wordclass": "", "def_blocks": [{"header_num": " <span class=\"reference_qualification\">das Bett f\u00fcr die Nacht vorbereiten (aufsch\u00fctteln, evtl. neu beziehen usw.)</span>", "example": "Kannst du noch die Betten in Zimmer 513 machen?"}]}], [{"headword": "fre\u0331i\u0331 ma<span class=\"separator\">\u00b7</span>chen", "wordclass": "<acronym title=\"verb\">VB</acronym>", "separator": "\u00b7", "verbclass": "<acronym title=\"with object\">with obj</acronym>", "def_blocks": [{"header_num": "1. ", "grammatical_construction": "<acronym title=\"jemand\">jd</acronym> macht frei", "definition": "dienstfrei nehmen; Freizeit beanspruchen", "example": "Sie sagte, sie wolle nach Abgabe der Arbeit einige Tage freimachen."}, {"header_num": "2. ", "grammatical_construction": "<acronym title=\"jemand\">jd</acronym> macht etwas frei", "style": "<acronym title=\"synonym\">\u2248</acronym> entbl\u00f6\u00dfen", "definition": "verwendet als Aufforderung des Arztes an den Patienten, sich (teilweise) zu entkleiden", "example": "Bitte machen Sie den Oberk\u00f6rper frei!"}]}, {"headword": "fre\u0331i\u0331 ma<span class=\"separator\">\u00b7</span>chen", "wordclass": "<acronym title=\"verb\">VB</acronym>", "separator": "\u00b7", "verbclass": "<acronym title=\"with 'sich'\">with SICH</acronym>", "def_blocks": [{"header_num": " <span class=\"reference_qualification\">siehe aber auch</span> <span class=\"indirect_reference_RQ\"> frei</span>", "idiom_proverb": "<acronym title=\"jemand\">jd</acronym> macht sich frei (f\u00fcr etwas <span class=\"case\"><acronym title=\"accusative\">acc</acronym></span>)", "case": "<acronym title=\"accusative\">acc</acronym>", "style": "<acronym title=\"informal\">inf</acronym>", "definition": "<acronym title=\"jemand\">jd</acronym> nimmt sich Zeit f\u00fcr etwas"}]}], [{"headword": "be<span class=\"separator\">\u00b7</span>wu\u0323sst ma<span class=\"separator\">\u00b7</span>chen", "wordclass": "<acronym title=\"verb\">VB</acronym>", "separator": "\u00b7", "flexion": "<machst bewusst, machte bewusst, hat bewusstgemacht>", "verbclass": "<acronym title=\"with object\">with obj</acronym>", "def_blocks": [{"header_num": "", "idiom_proverb": "<acronym title=\"jemand\">jd</acronym> macht <acronym title=\"jemandem\">jdm</acronym> etwas <span class=\"case\"><acronym title=\"accusative\">acc</acronym></span> bewusst", "case": "<acronym title=\"accusative\">acc</acronym>", "sense": "(klarmachen, ins Bewusstsein rufen)", "example": "Er versuchte, ihr das Problem bewusstzumachen."}]}]]